<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/grey4">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Third Year :"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        android:id="@+id/thirdyear"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="bold"
        android:gravity="center_horizontal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Somesh"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/somesh"
        android:layout_below="@+id/thirdyear"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/somesh_num"
        android:layout_below="@+id/thirdyear"
        android:layout_toRightOf="@+id/somesh"
        android:layout_toEndOf="@+id/somesh"
        android:text="+91 9674642046"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Adarsh"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/adarsh"
        android:layout_below="@+id/somesh"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/adarsh_num"
        android:layout_below="@+id/somesh"
        android:layout_toRightOf="@+id/adarsh"
        android:layout_toEndOf="@+id/adarsh"
        android:text="+91 7076316399"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bishakh"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/bishakh"
        android:layout_below="@+id/adarsh"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/bishakh_num"
        android:layout_below="@+id/adarsh"
        android:layout_toRightOf="@+id/bishakh"
        android:layout_toEndOf="@+id/bishakh"
        android:text="+91 9474649114"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ananya"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/ananya"
        android:layout_below="@+id/bishakh"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/ananya_num"
        android:layout_below="@+id/bishakh"
        android:layout_toRightOf="@+id/ananya"
        android:layout_toEndOf="@+id/ananya"
        android:text="+91 7063495554"
        android:textColor="#FFFFFF" />

</RelativeLayout>
