<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/grey4">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Second Year"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        android:id="@+id/secondyear"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:gravity="center_horizontal"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Vivek"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/vivek"
        android:layout_below="@+id/secondyear"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/vivek_num"
        android:layout_below="@+id/secondyear"
        android:layout_toRightOf="@+id/vivek"
        android:layout_toEndOf="@+id/vivek"
        android:text="+91 9800238139"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Amit"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/amit"
        android:layout_below="@+id/vivek"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/amit_num"
        android:layout_below="@+id/vivek"
        android:layout_toRightOf="@+id/amit"
        android:layout_toEndOf="@+id/amit"
        android:text="+91 8768401855"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Devansh"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/devansh"
        android:layout_below="@+id/amit"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/devansh_num"
        android:layout_below="@+id/amit"
        android:layout_toRightOf="@+id/devansh"
        android:layout_toEndOf="@+id/devansh"
        android:text="+91 9674825450"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deeksha"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/deeksha"
        android:layout_below="@+id/devansh"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/deeksha_num"
        android:layout_below="@+id/devansh"
        android:layout_toRightOf="@+id/deeksha"
        android:layout_toEndOf="@+id/deeksha"
        android:text="+91 9083583214"
        android:textColor="#FFFFFF" />

</RelativeLayout>
