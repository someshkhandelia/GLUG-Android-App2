<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/grey4">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Final Year :"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:id="@+id/finalyear"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textStyle="bold"
        android:gravity="center_horizontal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Surya"
        android:textSize="15dp"
        android:ems="10"
        android:id="@+id/surya"
        android:layout_below="@+id/finalyear"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textSize="15sp"
        android:ems="10"
        android:id="@+id/surya_num"
        android:layout_below="@+id/finalyear"
        android:layout_toRightOf="@+id/surya"
        android:layout_toEndOf="@+id/surya"
        android:text="+91 9614826204"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neetesh"
        android:textSize="15sp"
        android:ems="10"
        android:id="@+id/neetesh"
        android:layout_below="@+id/surya"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:ems="10"
        android:id="@+id/neetesh_num"
        android:layout_below="@+id/surya"
        android:layout_toRightOf="@+id/neetesh"
        android:layout_toEndOf="@+id/neetesh"
        android:text="+91 8670864497"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Arpit Singh"
        android:textSize="15sp"
        android:ems="10"
        android:id="@+id/arpitsingh"
        android:layout_below="@+id/neetesh"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:ems="10"
        android:id="@+id/arpitsingh_num"
        android:layout_below="@+id/neetesh"
        android:layout_toRightOf="@+id/arpitsingh"
        android:layout_toEndOf="@+id/arpitsingh"
        android:text="+91 9832821697"
        android:textColor="#FFFFFF" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Drishti"
        android:textSize="15sp"
        android:ems="10"
        android:id="@+id/drishti"
        android:layout_below="@+id/arpitsingh"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="#FFFFFF" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:ems="10"
        android:id="@+id/drishti_num"
        android:layout_below="@+id/arpitsingh"
        android:layout_toRightOf="@+id/drishti"
        android:layout_toEndOf="@+id/drishti"
        android:text="+91 8820508067"
        android:textColor="#FFFFFF" />

</RelativeLayout>
